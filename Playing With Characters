#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define MAX_LEN 1000
int main() 
{

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    char ch;  
    char s[MAX_LEN];
    char sen[MAX_LEN]; 
    scanf("%c", &ch);
    printf("%c\n", ch);
    scanf("%s", s);
    printf("%s\n", s);
    scanf("\n");
    scanf("%[^\n]%*c", sen);
    printf("%s\n",sen);
    return 0;
}

